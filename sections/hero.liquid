


<div id="{{ section.settings.anchor-id }}" class="hero bg-color-{{ section.settings.bg-color }} {{ section.settings.visibility }}">
<style>
  .hero,
  .hero-content-layout {
    {% if section.settings.img-ratio == "square" %}
      min-height: calc(50vw);
    {% endif %}
    {% if section.settings.img-ratio == "portrait" %}
      min-height: calc(50vw * 1.33);
    {% endif %}
    {% if section.settings.img-ratio == "portrait-sm" %}
      min-height: calc(50vw * 1.15);
    {% endif %}
    {% if section.settings.img-ratio == "landscape" %}
      min-height: calc(50vw * 0.75);
    {% endif %}
    {% if section.settings.img-ratio == "landscape-wide" %}
      min-height: calc(50vw * 0.56);
    {% endif %}
    {% if section.settings.img-ratio == "banner" %}
      min-height: calc(50vw * 0.20);
    {% endif %}
  }

  @media screen and (max-width: 767px) {
    .hero,
    .hero-content-layout {
      min-height: auto;
      height: auto;
    }
    .hero-second-img {
      display:none;
    }
  }
</style>
  <div class="hero-layout">



    <div class="hero-content-wrapper fade-in-{{ section.settings.fade-content }}" {% if section.settings.layout=='img-text' %}style="margin-left:auto;"{% endif %}>
      <div class="page-padding" style="min-height:100%">
        <div class="container-large w-container" style="min-height:100%">
          <div class="hero-content-layout valign-{{ section.settings.valign }} text-align-{{ section.settings.text-alignment }}">



            <div style="order:2;padding-bottom:20px;width:100%" class="padding-top padding-{{ section.settings.padding-top }}" style="min-height:100%">
              <div class="heading-layout" style="min-height:100%">

                {%- for block in section.blocks -%}
                  {%- if block.settings.position != "btm" -%}
                    {%- case block.type -%}

                      {%- when 'title' -%}
                        <div class="padding-top padding-{{ block.settings.padding-top }}">
                          <div class="padding-bottom padding-{{ block.settings.padding-bottom }}">
                            {% if block.settings.link != "" %}
                              <a href="{{ block.settings.link }}">
                              {% endif %}
                              <{{ block.settings.tag }} class="heading-{{ block.settings.title-size }} text-style-{{ block.settings.style }}">
                                {{ block.settings.text }}
                              </{{ block.settings.tag }}>
                              {% if block.settings.link != "" %}
                              </a>
                            {% endif %}
                          </div>
                        </div>

                      {%- when 'tagline' -%}

                        <div class="padding-top padding-{{ block.settings.padding-top }}">
                          <div class="padding-bottom padding-{{ block.settings.padding-bottom }}">
                            {% if block.settings.link != "" %}
                              <a href="{{ block.settings.link }}" class="heading-tag w-inline-block">
                              {% else %}
                                <div class="heading-tag w-inline-block">
                                {% endif %}
                                <div class="text-tag text-style-{{ block.settings.style }}">
                                  {{ block.settings.text }}
                                </div>
                                {% if block.settings.link != "" %}
                                </a>
                            {% else %}
                              </div>
                            {% endif %}
                          </div>
                        </div>


                      {%- when 'paragraph' -%}

                        <div class="padding-top padding-{{ block.settings.padding-top }}">
                          <div class="padding-bottom padding-{{ block.settings.padding-bottom }}">
                            <div class="rich-text text-style-{{ block.settings.style }} rich-text_paragraph is-{{ block.settings.size }} {% if block.settings.dropcap == true %}drop-cap{% endif %}">
                              {{ block.settings.text }}
                            </div>
                          </div>
                        </div>


                      {%- when 'link' -%}

                        <div class="padding-top padding-{{ block.settings.padding-top }}">
                          <div class="padding-bottom padding-{{ block.settings.padding-bottom }}">
                            <a class="{{ block.settings.style }} w-button" href="{{ block.settings.link }}">
                              {{block.settings.text }}
                            </a>
                          </div>
                        </div>

                      {%- when 'image' -%}

                        <div class="padding-top padding-{{ block.settings.padding-top }}">
                            <div class="padding-bottom padding-{{ block.settings.padding-bottom }}">
                            <div style="width:{{ block.settings.width }}%;" class="hero-second-img halign-{{ block.settings.halign }}">
                            
                                {% if block.settings.link != "" %}
                                <a href="{{ block.settings.link }}">
                                {% endif %}
                                <div class="img-{{ block.settings.img-ratio }}">
                                  <img class="img-base {% if block.settings.img-ratio == 'auto' %}is-auto{% endif %}" src="{{ block.settings.img | image_url: width:400 }}" loading="lazy" alt="{{ block.settings.img.alt | t}}">
                                </div>
                                {% if block.settings.link != "" %}
                                </a>
                                {% endif %}
                              </div>
                            </div>
                          </div>

                      {%- endcase -%}
                    {%- endif -%}
                  {%- endfor -%}
                </div>
              </div>

              {% if section.settings.hero-img %}
