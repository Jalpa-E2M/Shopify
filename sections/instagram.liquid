<div id="{{ section.settings.anchor-id }}"
  class="instagram bg-color-{{ section.settings.bg-color }} {{ section.settings.visibility }}">
  <div class="padding-top padding-{{ section.settings.padding-top }}">
    <div class="padding-bottom padding-{{ section.settings.padding-bottom }}">
      <div class="fade-in-{{ section.settings.fade }}">
        <div class="page-padding">
          <div class="container-{{ section.settings.container-width }} w-container">
            <div class="text-align-{{ section.settings.text-alignment }}">


              {%- for block in section.blocks -%}


              {%- case block.type -%}

              {%- when 'title' -%}
              <div class="padding-top padding-{{ block.settings.padding-top }}">
                <div class="padding-bottom padding-{{ block.settings.padding-bottom }}">
                  {% if block.settings.link != "" %}<a href="{{ block.settings.link }}" {% else %}<div {% endif %}
                    class="instagram_header">
                    <{{ block.settings.tag }}
                      class="heading-{{ block.settings.title-size }} text-style-{{ block.settings.style }}">
                      {{ block.settings.text }}
                    </{{ block.settings.tag }}>
                    {% if block.settings.link != "" %}
                  </a>{% else %}</div>{% endif %}
              </div>
            </div>

            {%- endcase -%}
            {%- endfor -%}

            <div class="instagram_wrapper {% if section.settings.carousel == true %}hide-mobile-landscape{% endif %}">
              <div class="instagram_images">

                {%- for block in section.blocks -%}


                {%- case block.type -%}

                {%- when 'instagram-images' -%}

                <a href="{{ block.settings.link }}" class="instagram_img-link-wrapper w-inline-block">
                  <div class="img-portrait-sm">
                    <img src="{{ block.settings.image | image_url: width: 400 }}" loading="lazy" alt=""
                      class="img-base">
                  </div>

                </a>

                {%- endcase -%}
                {%- endfor -%}

              </div>
            </div>
            {% if section.settings.carousel == true %}
            <div class="list_wrapper is-swiper show-mobile-landscape">
              <div class="swiper is-list">
                <div class="list_layout is-{{ section.settings.columns }}-col swiper-wrapper">

                  {%- for block in section.blocks -%}
                  {%- case block.type -%}
                  {%- when 'instagram-images' -%}
                  <a href="{{ block.settings.link }}" class="swiper-slide list_item of-5 w-inline-block">
                    <div class="img-portrait-sm"><img src="{{ block.settings.image | image_url: width: 400 }}"
                        loading="lazy" alt="" class="img-base"></div>
                  </a>
                  {%- endcase -%}
                  {%- endfor -%}


                </div>
              </div>
              <div class="swiper-button-left is-shop-list"></div>
              <div class="swiper-button-next is-shop-list"></div>
              <div class="swiper-pagination"></div>
              {% comment %} <div class="hidden w-embed w-script">
                <script>
                  currentSwiper = document.currentScript.parentNode.parentNode.firstChild.nextElementSibling;
                  navNext = currentSwiper.nextElementSibling.nextElementSibling;
                  navPrev = currentSwiper.nextElementSibling;
                  pagination = currentSwiper.nextElementSibling.nextElementSibling.nextElementSibling;
                  var swipername = new Swiper(currentSwiper, {
                    slidesPerView: "auto",
                    slidesPerGroupAuto: false,
                    speed: 250,
                    //spaceBetween: 45,
                    loop: true,
                    initialSlide: 0,
                    centeredSlides: false,
                    shortSwipes: true,
                    threshold: 20,
                    passiveListeners: true,
                    mousewheel: {
                      releaseOnEdges: false,
                      forceToAxis: true,
                      sensitivity: 0.5,
                    },
                    navigation: {
                      nextEl: navNext,
                      prevEl: navPrev,
                    },
                    pagination: {
                      el: pagination,
                      type: 'bullets',
                      clickable: 'false'
                    },
                  });
                </script>
              </div> {% endcomment %}
            </div>
            {% endif %}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>


