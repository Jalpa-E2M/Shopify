                       <div class="klaviyo-form-VmCUDV"></div>
                      </div>
                    </div>


                    {% if section.settings.logomark != blank %}
                      <div class="footer_logomark hide-tablet">
                      <img src="{{ section.settings.logomark | img_url: '400x' }}" loading="lazy" alt="{{ section.settings.logomark.alt | t }}">
                      </div>
                    {% endif %}

                  </div>


                  <div class="footer_btm">
                    <div class="footer_links-wrapper">

                    {% for link in linklists.footer.links %}
                      {% if link.links.size > 0 %}
                        <div class="footer_links-col">
                          {% for child_link in link.links %}
                            <a href="{{ child_link.url }}" class="footer_link">{{ child_link.title }}</a>
                          {% endfor %}
                        </div>
                      {% endif %}
                    {% endfor %} 


                      <div id="w-node-_56f5f551-696c-c63f-5d70-990ba4871550-a4871516" class="footer_links-col is-copyright">
                        {% if section.settings.logomark != blank %}
                          <div class="footer_logomark show-tablet"><img src="{{ section.settings.logomark | image_url: width: 500 }}" loading="lazy" alt="{{ section.settings.logomark.alt | t }}"></div>
                        {% endif %}
                        <div class="footer_caption">{{ section.settings.copyright }}</div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>


{% schema %}
    {
        "name": "Footer",
        "tag": "div",
        "settings": [
        {
          "type": "richtext",
          "id": "top-text",
          "label": "Top Text",
          "default": "<p>Sign up for exclusive product drops, restocks, interior design content, recipes, and much more before anyone else.</p>"
        },
        {
          "type": "text",
          "id": "form-caption",
          "label": "Form Caption",
          "default": "EMAIL US ANYTIME AT: INFO@EYE-SWOON.COM"
        },
        {
          "type": "text",
          "id": "copyright",
          "label": "Copyright Text",
          "default": "Â© 2022 EYESWOON"
        },
        {
            "type": "select",
            "id": "bg-color",
            "options": [
            {
                "value": "white",
                "label": "White"
            },
            {
                "value": "extralight",
                "label": "Extra Light"
            },
            {
                "value": "light",
                "label": "Light"
            },
            {
                "value": "medium",
                "label": "Medium"
            },
            {
                "value": "dark",
                "label": "Dark"
            },
            {
                "value": "extradark",
                "label": "Extra Dark"
            }
            ],
            "default": "medium",
            "label": "Bg Color"
        },
        {
          "type": "image_picker",
          "id": "featured_img",
          "label": "Featured Image"
        },
        {
          "type": "image_picker",
          "id": "logomark",
          "label": "Logomark"
        },
        {
  "type": "select",
  "id": "container-width",
  "options": [
    {
      "value": "small",
      "label": "Small"
    },
    {
      "value": "medium",
      "label": "Medium"
    },
    {
      "value": "large",
      "label": "Large"
    },
    {
      "value": "xlarge",
      "label": "X Large"
    },
    {
      "value": "none",
      "label": "No container"
    }
  ],
  "default": "large",
  "label": "Container Width"
},
{
  "type": "select",
  "id": "fade",
  "options": [
    {
      "value": "0",
      "label": "None"
    },
    {
      "value": "1",
      "label": "First (Fast)"
    },
    {
      "value": "2",
      "label": "Second (Medium)"
    },
    {
      "value": "3",
      "label": "Third (Slow)"
    }
  ],
  "default": "2",
  "label": "Content Fade-in"
},
{
  "type": "text",
  "id": "anchor-id",
  "label": "Anchor ID",
  "info": "Useful for linking to this part of the page"
}
        ]
    }
{% endschema %}
