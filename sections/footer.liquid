<div id="{{ section.settings.anchor-id }}" class="footer wf-section bg-color-{{ section.settings.bg-color }} fade-in-{{ section.settings.fade }}">
        <div class="padding-medium padding-vertical">
          <div class="page-padding">
            <div class="container-{{ section.settings.container-width }} w-container">
              <div class="footer_layout">
                  {% if section.settings.featured_img != blank %}
                  <div class="footer_img">
                    <div class="img-auto">
                    <img src="{{ section.settings.featured_img | img_url: '1000x' }}" loading="lazy" class="img-base is-auto" alt="{{ section.settings.featured_img.alt | t }}">
                    </div>
                  </div>
                  {% endif %}
                  
                <div class="footer_top-btm-wrapper">
                  <div class="footer_top" style="width:100%;">
                    <div class="footer_subscribe" style="width:100%;">
                      {% if section.settings.top-text != blank %}
                        <div class="body-medium">{{ section.settings.top-text }}</div>
                      {% endif %}
                      <div id="footer-form" class="footer_form-block w-form" style="width:100%;margin-top:0;"> 
